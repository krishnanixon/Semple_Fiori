			<mvc:XMLView id="beginView" viewName="project1.view.List"/>
		</beginColumnPages>
        <midColumnPages>
			<mvc:XMLView id="detailView" viewName="project1.view.Detail"/>
		</midColumnPages>
	</FlexibleColumnLayout>
